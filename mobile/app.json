{
  "expo": {
    "name": "ForgeAI Builder",
    "slug": "forgeai-builder",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "forgeai",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.forgeai.builder"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0F172A"
      },
      "package": "com.forgeai.builder"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "apiBaseUrl": "https://api.forgeai.dev",
      "appEnv": "development",
      "supabaseUrl": "https://placeholder.supabase.co",
      "supabaseAnonKey": "placeholder-anon-key"
    }
  }
}
